#!/bin/bash
#
# Generate instance data for OPTs.
#
default_run_id=run_009

if [[ $# -ne 1 ]]; then
	echo "Using default run $default_run_id"
	run_id=$default_run_id
else
	run_id=$1
fi

lib_repo=S2-models-demo
out_repo=S2-test-data

s2_linux_x86 data instances -s $lib_repo/opts -o $out_repo/synth -f $run_id -n 10 -t S2-terminology --formulas S2-data-synth-formulas --usePlaceholderData --log $out_repo/logs/`basename $0`.log
