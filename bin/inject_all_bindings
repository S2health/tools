#!/bin/bash
#
# command to be executed from root directory containing S2 Health cloned repos.
#

lib_id=S2-models-demo

terminologies=( loinc rxnorm openehr s2 snomed ucum )

for i in "${terminologies[@]}" ; do
	echo "adlc -q --cfg tools/cfg/adlc.cfg -b $lib_id --inject_term_bindings $i -i $lib_id/injection/${i}_bindings.csv > $lib_id/logs/inject_${i}.log"
	adlc -q --cfg tools/cfg/adlc.cfg -b $lib_id --inject_term_bindings $i -i $lib_id/injection/${i}_bindings.csv > $lib_id/logs/inject_${i}.log 2>&1
done

