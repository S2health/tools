#!/bin/bash
#
# model command to be executed from above the directory 'Graphite', containing Graphite Health cloned repos.
#

lib_id=S2-models-demo
terminology=loinc

echo "adlc -q --cfg cfg/adlc.cfg -b $lib_id --inject_term_bindings ${terminology} -i $lib_id/injection/${terminology}_bindings.csv > $lib_id/logs/inject_${terminology}.log 2>&1"
adlc -q --cfg cfg/adlc.cfg -b $lib_id --inject_term_bindings ${terminology} -i $lib_id/injection/${terminology}_bindings.csv > $lib_id/logs/inject_${terminology}.log 2>&1

