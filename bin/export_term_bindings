#!/bin/bash
#
# model command to be executed in rot directory containing S2 Health cloned repos.
#

lib_id=S2-models-demo
echo 'adlc --cfg tools/cfg/adlc.cfg -b $lib_id --export_term_bindings -o $lib_id/term_bindings'
adlc --cfg tools/cfg/adlc.cfg -b $lib_id --export_term_bindings -o $lib_id/term_bindings > $lib_id/logs/`basename $0`.log 2>&1
